{
  "name": "@iexec/iapp-maker",
  "version": "0.0.1",
  "description": "A CLI in order to build, test and deploy an iexec application",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node --env-file=.env ./src/index.js",
    "dev": "node --env-file=.env --watch ./src/index.js",
    "dev:pretty": "node --env-file=.env --watch ./src/index.js | pino-pretty -tc",
    "check-format": "prettier --check .",
    "format": "prettier --write ."
  },
  "author": "iExec",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iExecBlockchainComputing/iapp.git"
  },
  "keywords": [
    "iExec"
  ],
  "bugs": {
    "url": "https://github.com/iExecBlockchainComputing/iapp/issues"
  },
  "homepage": "https://github.com/iExecBlockchainComputing/iapp/blob/main/cli/README.md",
  "files": [
    "src",
    "templates"
  ],
  "dependencies": {
    "cls-hooked": "^4.2.2",
    "dockerode": "^4.0.2",
    "ethers": "^6.13.4",
    "express": "^5.0.1",
    "iexec": "^8.12.0",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cls-hooked": "^4.3.9",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.3.3"
  }
}
