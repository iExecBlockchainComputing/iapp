{
  "name": "@iexec/iapp",
  "version": "0.0.0",
  "description": "A SDK to guide you through the process of managing and running an iExec iApp",
  "main": "./dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "homepage": "https://github.com/iExecBlockchainComputing/iapp/blob/main/cli/README.md",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "logo-iexec.png",
    "src",
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "check-types": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier --write \"(src|tests)/**/*.ts\"",
    "check-format": "prettier --check \"(src|tests)/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iExecBlockchainComputing/iapp.git"
  },
  "keywords": [
    "iExec"
  ],
  "author": "iExec",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/iExecBlockchainComputing/iapp/issues"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/random": "^5.7.0",
    "ethers": "^6.13.2",
    "graphql-request": "^6.0.0",
    "iexec": "^8.18.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-sonarjs": "^0.21.0",
    "prettier": "^2.8.4",
    "typescript": "^5.5.2"
  }
}
